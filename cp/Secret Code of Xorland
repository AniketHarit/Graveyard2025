#include<bits/stdc++.h>
using namespace std;
int xor1(vector<int> a,int k){
    int xr=0;
    int count=0;
    map<int,int> mpp;
    mpp[xr]++;
    for(int i=0;i<a.size();i++){
        xr=xr^a[i];
        int x=xr^k;
        count+=mpp[x];
        mpp[xr]++;
    }
    return count;
}
int main(){
    int n;
    cin>>n;
    vector<int> vec1(n);
    for(int i=0;i<n;i++){
        cin>>vec1[i];
    }
    int target;
    cin>>target;
    int ans=xor1(vec1,target);
    cout<<ans;

}
